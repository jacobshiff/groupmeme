<div class="navbar" role="navigation">
  <!-- Brand and toggle get grouped for better mobile display -->
  <div class="navbar-header">
    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#groupslam-navbar-collapse-1" aria-expanded="false">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a class="navbar-brand" href="/"><img style="width:80%" src="https://s3.amazonaws.com/groupmeme/Group+Slam+Logo-01.png"></a>
  </div>

  <div class="collapse navbar-collapse" id="groupslam-navbar-collapse-1">
    <p class="navbar-text"><%= current_group.title if current_group%></p>
    <ul class="nav navbar-nav navbar-right">
      <% if !!session[:user_id] %>
      <%= render 'layouts/shared/user' %>
      <% else %>
      <%= render 'layouts/shared/public' %>
      <% end %>
    </ul>
  </div><!-- /.navbar-collapse -->
</div>

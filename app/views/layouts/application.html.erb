<!DOCTYPE html>
<html>
<head>
  <title>GroupMeme</title>
  <%= stylesheet_link_tag 'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
  <!-- Favicon -->
  <link rel="icon" 
      type="image/png" 
      href="https://s3.amazonaws.com/groupmeme/Group+Slam+Favicon-02.png" />
</head>
<body>
<!-- Navbar -->
<%= render 'layouts/shared/basic_nav' %>

<!-- Flashes -->
<% flash.each do |type, message| %>
    <div class="alert alert-<%= type %>" role="alert"><%= message %></div>
  <% end %>
<div class="container">
<%= yield %>
</div>
</body>
</html>
